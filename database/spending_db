CREATE TABLE spending_db
(
    spending_id serial not null PRIMARY KEY,
    amount money,
    month VARCHAR (15),
	date date,
    user_id integer,
	 CONSTRAINT fk_users FOREIGN KEY (user_id) REFERENCES users_db (user_id),
    category_id integer,
	 CONSTRAINT fk_category FOREIGN KEY (category_id) REFERENCES category_db (category_id));
