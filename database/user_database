
CREATE TABLE IF NOT EXISTS public."Userss"
(
    user_id integer NOT NULL,
    user_name character varying(30) COLLATE pg_catalog."default",
    first_name character varying(30) COLLATE pg_catalog."default",
    last_name character varying(30) COLLATE pg_catalog."default",
    email character varying(30) COLLATE pg_catalog."default",
    phone_number character varying(20) COLLATE pg_catalog."default",
    nick_name character varying(20) COLLATE pg_catalog."default",
    typeof_user character varying(15) COLLATE pg_catalog."default",
    CONSTRAINT "Users" PRIMARY KEY (user_id)
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public."Userss"
    OWNER to postgres;

GRANT INSERT, UPDATE ON TABLE public."Userss" TO "Regular User";

GRANT ALL ON TABLE public."Userss" TO "Super User";

GRANT ALL ON TABLE public."Userss" TO postgres;
